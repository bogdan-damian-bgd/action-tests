name: Test Python getenv
on:
  workflow_dispatch:

jobs:
  test:
    runs-on: windows-2022
    name: Test
    steps:
      - shell: powershell
        run: |
          New-Item tscript.py
          "import os" >> tscript.py
          "print getenv('APPDATA')" >> tscript.py
          python.exe tscript.py
